{
  "mcpServers": {
    "home-assistant": {
      "command": "mcp-proxy",
      "args": [
        "https://mithras.duckdns.org:8443/mcp_server/sse"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone", "America/Los_Angeles"]
    },
    "searxng": {
      "command": "npx",
      "args": ["-y", "mcp-searxng"]
    }
  }
}
